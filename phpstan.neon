parameters:
    level: 6
    paths:
        - src
    ignoreErrors:
        - '#Access to an undefined property .+::\$[a-zA-Z_]+#'
        - '#Call to an undefined static method .+::create\(\)#'
        - '#Call to an undefined static method .+::where\(\)#'
        - '#Method .+ never returns null so it can be removed from the return type.#'